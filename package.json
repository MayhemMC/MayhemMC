{
    "name": "photon-pwa",
    "version": "1.0.0",
    "description": "Create single page applications using PhotonCSS",
    "entry": "index.js",
    "scripts": {
        "build": "webpack --config webpack.prod.js",
        "dev-api": "env NODE_ENV=dev nodemon .",
        "dev-webpack": "webpack-dev-server --config webpack.dev.js",
        "dev": "concurrently --kill-others -p \"[{name}]\" -n \"WEBPACK,EXPRESS\" -c \"blue,magenta\" \"npm:dev-webpack\" \"npm:dev-api\"",
        "serve": "node ."
    },
